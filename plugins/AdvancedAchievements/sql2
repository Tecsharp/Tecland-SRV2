CREATE TABLE achievements (
	playername char(36),
	achievement varchar(64),
	date varchar(250),
	PRIMARY KEY (playername)
);
CREATE TABLE advancementscompleted (
	playername char(36),
	advancementscompleted int,
	PRIMARY KEY (playername)
);
